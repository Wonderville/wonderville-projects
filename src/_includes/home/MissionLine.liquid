<article class="MissionLine">
  <a class="MissionLine-link" href="/programs#{{ anchor }}">
    <img
      class="MissionLine-image"
      src="{{ src }}"
      alt="{{ alt }}"
    >

    <h1 class="MissionLine-name">
      {{ name }}
    </h1>

    <p class="MissionLine-text Box">
      {{ body }}
    </p>
  </a>
</article>